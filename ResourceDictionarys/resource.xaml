<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:gregor_bracko_pregledovalnikOglasov.ResourceDictionarys">

    <Style TargetType="Button" x:Key="GumbDodaj">
        <Setter Property="Background" Value="DeepSkyBlue"/>
        <Setter Property="Padding" Value="5"/>
    </Style>

    <Style TargetType="ListView" x:Key="ListView">
        <Setter Property="Background" Value="#FFC6E2EB"/>
        <Setter Property="Padding" Value="5"/>
    </Style>

    <Style TargetType="Label" x:Key="Label">
        <Setter Property="FontStyle" Value="Italic"/>
    </Style>
</ResourceDictionary>